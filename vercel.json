{
  "framework": "vite",
  "redirects": [
    { "source": "/home", "destination": "/", "permanent": true },
    { "source": "/work", "destination": "/", "permanent": true }
  ],
  "rewrites": [
    { "source": "/((?!api/).*)", "destination": "/" }
  ],
  "functions": {
    "api/book-suggestion.js": {
      "runtime": "nodejs20.x",
      "includeFiles": "node_modules/@sanity/client/**,node_modules/resend/**"
    }
  }
}



